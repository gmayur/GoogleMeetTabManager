{
    "manifest_version": 3,
    "homepage_url": "https://github.com/gmayur/GoogleMeetTabManager",
    "name": "Google Meet Tab Manager",
    "version": "1.0",
    
    "description": "Manages and closes inactive Google Meet landing tabs",
    "permissions": [
      "storage",
      "alarms",
      "tabs"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      },
      "default_title": "Google Meet Tab Manager"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }

  }